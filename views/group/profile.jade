#breadcrumbs
  != partial('breadcrumbs', {collection: crumbs, as: 'crumb'})
  
#group-profile
  h1= group.name

  h2 About
  != gdescription

  ul.profile
    li
      span.label Website:&nbsp;
      - if (typeof website != "undefined")
        span
          a(href=website)= website
      - else
        span (not listed)
        
    li
      span.label Created On:&nbsp;
      span= datetime(group.created_at)

    li
      span.label Owners:&nbsp;
      ul
        - each owner in owners
          li
            a(href="/player/profile/"+encodeURI(owner.aliases[0])+"@"+encodeURI(owner.handle))
              span.alias= owner.aliases[0]
              span.handle= "@"+owner.handle

    li
      span.label Members:&nbsp;
      ul
        li

  h2 Events
  ul.profile
    li

  h2 Tournaments
  ul.profile
    li

  h2 Ladders
  ul.profile
    li
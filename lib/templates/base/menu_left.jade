- each menu in menus
  - if (menu.collapse){
    .menu-outer.collapse
  - } else {
    .menu-outer.expand
  - }
    .menu-header
      span.icon
      #{menu.title}
    ul.menu-inner
      - each item in menu.items
        li(title=item.description)
          span.menu-bullet &raquo;
            a(href=item.href) item.title
